<div id="note<%=note.id%>" class="note">
	<div class="delete"><%= link_to image_tag("icons/delete.png"), patient_patient_note_path(note.patient_id, note), :confirm => 'Are you sure?', :method => :delete, :remote => true %></div>
	<div class="timestamp"><%=l note.created_at, :format => :long %></div>
	<div class="content"><%=note.notes%></div>
</div>